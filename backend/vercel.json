{
  "version": 2,
  "buildCommand": "npm install",
  "outputDirectory": ".",
  "framework": "express",
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "api/index.js": {
      "runtime": "@vercel/node@3.1.1",
      "memory": 1024,
      "maxDuration": 60
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.js"
    }
  ]
}
